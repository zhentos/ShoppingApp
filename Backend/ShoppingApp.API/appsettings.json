{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "Properties": {
      "Application": "ShoppingApp"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "" // if not local db file
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [ "http://localhost:5173/" ] // shopping app local address
}
